{
  "name": "vuejs-typescript-aspnetcore-sample",
  "version": "1.0.0",
  "description": "A Vue.js with TypeScript application running on ASP.NET Core",
  "author": "Justin Yoo",
  "private": true,
  "scripts": {
    "dev": "node server",
    "build": "npm run build:server && npm run build:client && cross-env NODE_ENV=production node server",
    "build:ts": "tsc -p .",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "class-transformer": "^0.1.7",
    "compression": "^1.7.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "object-path": "^0.11.4",
    "v-mask": "^1.3.0",
    "vee-validate": "^2.0.0-rc.14",
    "vue": "^2.5.3",
    "vue-class-component": "^6.1.0",
    "vue-facebook-signin-button": "^1.0.1",
    "vue-form-wizard": "^0.6.4",
    "vue-google-signin-button": "^1.0.2",
    "vue-js-cookie": "^2.0.0",
    "vue-lazy-image": "^1.2.0",
    "vue-meta": "^1.2.0",
    "vue-progressbar": "^0.7.3",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.3",
    "vue-the-mask": "^0.11.0-alpha.2",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.0.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@types/moment": "^2.13.0",
    "@types/node": "^8.0.0",
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "compression-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.2.4",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.26.1",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^0.10.0",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "function-bind": "^1.1.0",
    "http-proxy-middleware": "^0.17.3",
    "inject-loader": "^2.0.1",
    "lodash.merge": "^4.6.0",
    "node-sass": "^4.6.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "ora": "^1.1.0",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "sw-precache-webpack-plugin": "^0.11.4",
    "ts-loader": "^2.0.1",
    "typescript": "2.5.1",
    "uglify-js": "^2.8.29",
    "url-loader": "^0.5.7",
    "vue-loader": "^12.2.1",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.3",
    "vue-typescript-import-dts": "^3.1.1",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^4.0.0",
    "webpack-node-externals": "^1.6.0"
  }
}
